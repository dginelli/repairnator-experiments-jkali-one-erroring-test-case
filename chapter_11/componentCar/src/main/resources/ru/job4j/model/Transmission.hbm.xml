<hibernate-mapping>
    <class name="ru.job4j.model.Transmission" table="transmision">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="type" column="type"/>
        <one-to-one name="car" class="ru.job4j.model.Car"/>
    </class>
</hibernate-mapping>