{
    "size":0,
    "query":{
        "bool":{
            "must":[
                {"term":{"%stype.raw":"fulltext"}},
                {"term":{"%sapplication.id.raw":"%s"}},
                {"match":{"%sprocess.final":"true"}},
                {"match":{"%sprocess.filters.scope":"Filter"}}
            ],
            "must_not":{
                "match":{"%sprocess.skip":"true"}
            },
            "filter": [
                {
                    "range":{
                        "%stimeStamp":{
                            "gte":%d,
                            "lte":%d,
                            "format":"epoch_millis"
                        }
                    }
                }
            ]
        }
    },
    "aggs":{
        "filterFields":{
            "terms":{
                "field":"%sprocess.filters.field.raw",
                "size":100,
                "order":{
                    "_count":"desc"
                }
            }
        }
    }
}